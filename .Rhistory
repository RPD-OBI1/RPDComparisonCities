roc <- which(names(dist.mat) == 'Rochester, New York')
roc.norm <-  (dist.mat[ , roc] - min(dist.mat[ , roc])) / (max(dist.mat[ , roc]) - min(dist.mat[ , roc]))
roc.perc <- round((1 - roc.norm)*100, 2)
roc.cities <- data.frame(City = names(dist.mat[, -1]), Similarity = roc.perc)
roc.cities.ordered <- roc.cities[order(roc.cities$Similarity, decreasing = T), ]
top20 <- data.frame(Rank = 1:20, roc.cities.ordered[2:21, ])
